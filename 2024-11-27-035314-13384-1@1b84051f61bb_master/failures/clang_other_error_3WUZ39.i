        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "sta stats missing!");
        return NL_SKIP;
    }
    if (nla_parse_nested (sinfo, NL80211_STA_INFO_MAX,
                          tb[NL80211_ATTR_STA_INFO],
                          stats_policy)) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to parse nested attributes!\n");
        return NL_SKIP;
    }

    if (sinfo[NL80211_STA_INFO_RX_BYTES] && sinfo[NL80211_STA_INFO_RX_PACKETS]) {
        guint32 rx_bytes = nla_get_u32 (sinfo[NL80211_STA_INFO_RX_BYTES]);
        devinfo->rx = (guint64) rx_bytes;
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "RX: %" "u" " bytes (%" "u" " packets)", rx_bytes, nla_get_u32 (sinfo[NL80211_STA_INFO_RX_PACKETS]));


    }
    if (sinfo[NL80211_STA_INFO_TX_BYTES] && sinfo[NL80211_STA_INFO_TX_PACKETS]) {
        guint32 tx_bytes = nla_get_u32 (sinfo[NL80211_STA_INFO_TX_BYTES]);
        devinfo->tx = (guint64) tx_bytes;
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "TX: %" "u" " bytes (%" "u" " packets)", tx_bytes, nla_get_u32 (sinfo[NL80211_STA_INFO_TX_PACKETS]));


    }
    if (sinfo[NL80211_STA_INFO_SIGNAL]) {
        int8_t dBm = (int8_t)nla_get_u8 (sinfo[NL80211_STA_INFO_SIGNAL]);
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "signal: %d dBm", dBm);
        devinfo->rssi = dBm;
        devinfo->qual = (((2 * ((int)dBm + 100)) > (100)) ? (100) : (((2 * ((int)dBm + 100)) < (1)) ? (1) : (2 * ((int)dBm + 100))));
    }
    if (sinfo[NL80211_STA_INFO_RX_BITRATE]) {
        char buf[100];
        parse_bitrate (sinfo[NL80211_STA_INFO_RX_BITRATE], buf, sizeof (buf));
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "rx bitrate: %s", buf);
        devinfo->rx_bitrate = g_strdup_inline (buf);
    }
    if (sinfo[NL80211_STA_INFO_TX_BITRATE]) {
        char buf[100];
        parse_bitrate (sinfo[NL80211_STA_INFO_TX_BITRATE], buf, sizeof (buf));
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "tx bitrate: %s", buf);
        devinfo->tx_bitrate = g_strdup_inline (buf);
    }
    if (sinfo[NL80211_STA_INFO_CONNECTED_TIME]) {
        devinfo->connected_time = nla_get_u32 (sinfo[NL80211_STA_INFO_CONNECTED_TIME]);
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "connected time: %" "u" " seconds", devinfo->connected_time);
    }

    return NL_SKIP;
}

static char *
channel_width_name (enum nl80211_chan_width width)
{
    switch (width) {
        case NL80211_CHAN_WIDTH_20_NOHT:
            return dcgettext (((void*)0), "20 MHz (no HT)", 5);
        case NL80211_CHAN_WIDTH_20:
            return dcgettext (((void*)0), "20 MHz", 5);
        case NL80211_CHAN_WIDTH_40:
            return dcgettext (((void*)0), "40 MHz", 5);
        case NL80211_CHAN_WIDTH_80:
            return dcgettext (((void*)0), "80 MHz", 5);
        case NL80211_CHAN_WIDTH_80P80:
            return dcgettext (((void*)0), "80+80 MHz", 5);
        case NL80211_CHAN_WIDTH_160:
            return dcgettext (((void*)0), "160 MHz", 5);
        case NL80211_CHAN_WIDTH_5:
            return dcgettext (((void*)0), "5 MHz", 5);
        case NL80211_CHAN_WIDTH_10:
            return dcgettext (((void*)0), "10 MHz", 5);
        default:
            return dcgettext (((void*)0), "unknown", 5);
    }
}

static int
ieee80211_frequency_to_channel (int freq)
{

    if (freq == 2484)
        return 14;
    else if (freq < 2484)
        return (freq - 2407) / 5;
    else if (freq >= 4910 && freq <= 4980)
        return (freq - 4000) / 5;
    else if (freq <= 45000)
        return (freq - 5000) / 5;
    else if (freq >= 58320 && freq <= 64800)
        return (freq - 56160) / 2160;
    else
        return 0;
}

static int
iface_cb (struct nl_msg *msg,
          void *arg)
{
    DevInfo *devinfo = (DevInfo*) arg;
    struct genlmsghdr *gnlh = nlmsg_data (nlmsg_hdr (msg));
    struct nlattr *tb_msg[NL80211_ATTR_MAX + 1];

    nla_parse (tb_msg, NL80211_ATTR_MAX, genlmsg_attrdata (gnlh, 0),
               genlmsg_attrlen (gnlh, 0), ((void*)0));

    if (tb_msg[NL80211_ATTR_MAC]) {
        memcpy (devinfo->hwaddr, nla_data (tb_msg[NL80211_ATTR_MAC]), 6);
    }

    if (tb_msg[NL80211_ATTR_SSID]) {
        char buf[((guint8) 0xff)];
        int len = nla_len (tb_msg[NL80211_ATTR_SSID]);
        memcpy (buf, nla_data (tb_msg[NL80211_ATTR_SSID]), len);
        buf [len] = '\0';
        devinfo->essid = g_strescape (buf, ((void*)0));
        g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "ssid: %s", buf);
    }

    if (tb_msg[NL80211_ATTR_WIPHY_FREQ]) {
        uint32_t freq = nla_get_u32 (tb_msg[NL80211_ATTR_WIPHY_FREQ]);
        char buf[100];
        char *pos = buf;

        pos += sprintf (pos, dcgettext (((void*)0), "%d (%d MHz)", 5),
                        ieee80211_frequency_to_channel (freq), freq);

        if (tb_msg[NL80211_ATTR_CHANNEL_WIDTH])
            sprintf (pos, dcgettext (((void*)0), ", width: %s", 5),
                     channel_width_name (nla_get_u32 (tb_msg[NL80211_ATTR_CHANNEL_WIDTH])));

        devinfo->channel = g_strdup_inline (buf);
    }

    return NL_SKIP;
}

void
get_wireless_info (DevInfo *devinfo)
{
    struct nl80211_state nlstate = {.sock = ((void*)0), .nl80211_id = -1};
    int err, ret;
    struct nl_msg *msg;
    signed long long devidx = 0;

    err = nl80211_init (&nlstate);
    if (err || (nlstate.nl80211_id < 0)) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to init netlink");
        return;
    }

    devidx = if_nametoindex (devinfo->name);


    msg = nlmsg_alloc ();
    if (!msg) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to allocate netlink message");
        goto cleanup;
    }
    genlmsg_put (msg, 0, 0, nlstate.nl80211_id, 0, (0x100|0x200), NL80211_CMD_GET_INTERFACE, 0);

    nla_put_u32 (msg, NL80211_ATTR_IFINDEX, devidx);

    nl_socket_modify_cb (nlstate.sock, NL_CB_VALID, NL_CB_CUSTOM, iface_cb, devinfo);

    ret = nl_send_auto (nlstate.sock, msg);
    g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "NL80211_CMD_GET_INTERFACE sent %d bytes to the kernel", ret);

    ret = nl_recvmsgs_default (nlstate.sock);
    nlmsg_free (msg);
    if (ret < 0) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to receive netlink message");
    }

    if (!devinfo->running)
        goto cleanup;


    msg = nlmsg_alloc ();
    if (!msg) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to allocate netlink message");
        goto cleanup;
    }
    genlmsg_put (msg, 0, 0, nlstate.nl80211_id, 0, (0x100|0x200), NL80211_CMD_GET_SCAN, 0);

    nla_put_u32 (msg, NL80211_ATTR_IFINDEX, devidx);

    nl_socket_modify_cb (nlstate.sock, NL_CB_VALID, NL_CB_CUSTOM, scan_cb, devinfo);

    ret = nl_send_auto (nlstate.sock, msg);
    g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "NL80211_CMD_GET_SCAN sent %d bytes to the kernel", ret);

    ret = nl_recvmsgs_default (nlstate.sock);
    nlmsg_free (msg);
    if (ret < 0) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to receive netlink message");
        goto cleanup;
    }

    if (!devinfo->running)
        goto cleanup;


    msg = nlmsg_alloc ();
    if (!msg) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to allocate netlink message");
        goto cleanup;
    }
    genlmsg_put (msg, 0, 0, nlstate.nl80211_id, 0, (0x100|0x200), NL80211_CMD_GET_STATION, 0);

    nla_put (msg, NL80211_ATTR_MAC, 6, devinfo->station_mac_addr);
    nla_put_u32 (msg, NL80211_ATTR_IFINDEX, devidx);

    nl_socket_modify_cb (nlstate.sock, NL_CB_VALID, NL_CB_CUSTOM, station_cb, devinfo);

    ret = nl_send_auto (nlstate.sock, msg);
    g_log (((gchar*) 0), G_LOG_LEVEL_DEBUG, "NL80211_CMD_GET_STATION sent %d bytes to the kernel", ret);

    ret = nl_recvmsgs_default (nlstate.sock);
    nlmsg_free (msg);
    if (ret < 0) {
        g_log (((gchar*) 0), G_LOG_LEVEL_WARNING, "failed to receive netlink message");
    }

 cleanup:
    nl80211_cleanup (&nlstate);
}
